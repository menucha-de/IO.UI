<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:havis.net.ui.shared.client.widgets">
	<ui:with type="havis.net.ui.shared.resourcebundle.ResourceBundle" field="res"></ui:with>
	<ui:style>
	.region {
		width: 8.8em;
		height: 32px;
		font-size: 1em;
	}
	</ui:style>
	<g:FlowPanel styleName="{res.css.row} {res.css.config}">
		<g:Label ui:field="label" addStyleNames="{res.css.label}"/>
		<g:ValueListBox ui:field="direction" addStyleNames="{style.region}"/>
		<g:ToggleButton ui:field="state" stylePrimaryName="webui-Switcher" />
		<s:ThreeStateSwitch ui:field="initial" />
	</g:FlowPanel>
</ui:UiBinder> 